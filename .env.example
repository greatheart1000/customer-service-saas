# Coze API Configuration
# Copy this file to .env and fill in your actual values

# Your Coze API token (required)
COZE_API_TOKEN=your_actual_api_token_here

# Your Bot ID (required for bot-related features)
COZE_BOT_ID=your_bot_id_here

# Your Workspace ID (required for dataset and workspace features)
COZE_WORKSPACE_ID=your_workspace_id_here

# API Base URL (optional, defaults to https://api.coze.cn)
COZE_API_BASE=https://api.coze.cn

# Debug mode (optional)
DEBUG=false